{
  "name": "ember-try",
  "version": "0.0.8",
  "description": "An ember-cli addon to test against multiple bower dependencies, such as ember and ember-data.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "client-test": "ember test",
    "node-test": "node_modules/mocha/bin/mocha .",
    "test": "npm run-script node-test && npm run-script client-test"
  },
  "repository": "https://github.com/kategengler/ember-try",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Katie Gengler",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "ember-cli": "0.2.7",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-babel": "^5.0.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.0",
    "ember-cli-htmlbars": "0.7.6",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.13",
    "ember-cli-uglify": "^1.0.1",
    "ember-export-application-global": "^1.0.2",
    "mocha": "^2.2.5",
    "should": "^7.0.1"
  },
  "keywords": [
    "ember-addon",
    "testing"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "core-object": "^1.1.0",
    "fs-extra": "^0.18.0",
    "promise-map-series": "^0.2.1",
    "resolve": "^1.1.6",
    "rimraf": "^2.3.2",
    "rsvp": "^3.0.17",
    "sync-exec": "^0.5.0"
  }
}
